    "\n",
    "df = pl.DataFrame({\"x\": [1,2], \"y\": [3,4]})\n",
    "Obsplot({\n",
    "    \"marks\": [\n",
    "        Plot.dot(df, {\"x\": \"x\", \"y\": \"y\"}),\n",
    "        Plot.dot(df, {\"x\": \"x\", \"y\": \"y\", \"fill\": \"red\"})\n",
    "    ]\n",
    "})"
   ]
  }
